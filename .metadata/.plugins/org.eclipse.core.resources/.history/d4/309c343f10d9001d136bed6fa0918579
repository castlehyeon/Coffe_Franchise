import java.util.Scanner;

import store.Headquarter;

public class HeadquarterMain {

	public static void main(String[] args) {
	
		Headquarter douzoneCoffee = new Headquarter();
		
		System.out.println("[관리자 모드로 로그인되었습니다.]");
		System.out.println("1.가맹점관리 2.메뉴관리 3.회원관리 4.스탬프관리");
		
		Scanner sc = new Scanner(System.in);
		int menuNum = sc.nextInt();
		 
		switch (menuNum) {
			case 1 : douzoneCoffee.manageStores();
			case 2 : douzoneCoffee.manageMenu();
			case 3 : douzoneCoffee.manageMembers();
			case 4 : douzoneCoffee.manageStamps();
			default :
		}
	
		
		
		
		
	}

}
