import menu.Menu;
import store.Headquarter;

public class Main {

	public static void main(String[] args) {

		Headquarter douzoneCoffee = new Headquarter();
		
		douzoneCoffee.createMenu(new Menu(1, "아메리카노", 1000));
		douzoneCoffee.createMenu(new Menu(2, "라떼", 2000));
		douzoneCoffee.createMenu(new Menu(1, "망고", 2500));
		
		douzoneCoffee.showMenuList();
		
		
	}

}
